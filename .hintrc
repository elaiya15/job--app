{
  "extends": [
    "development"
  ],
  "hints": {
    "no-protocol-relative-urls": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-synthesis",
          "-webkit-text-size-adjust",
          "width: fit-content"
        ]
      }
    ],
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 113"
  ]
}